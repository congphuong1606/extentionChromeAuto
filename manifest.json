{
	"manifest_version": 2,
 	"name": "Auto Price Remitano ",
 	"description": "Auto buy&sell Remitano - - @Author:PhuongCong - v1.01",
 	"version": "1.0",
 	"permissions": [
 		"http://*/*",
 		"https://*/*",
 		"tabs"
 	],
 	"browser_action": {
  		"default_icon" : "img/icon.png",
  		"default_popup": "popup.html"
	},
 	"icons": { 
  		"128" : "img/icon.png"
	},
	"background": {
		"scripts": ["js/app.js","js/fill-data.js"]
	}
}